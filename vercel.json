{
  "version": 2,
  "builds": [
    { "src": "api/verify.js", "use": "@vercel/node" },
    { "src": "api/generate.js", "use": "@vercel/node" },
    { "src": "api/stats.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/key", "dest": "/key.html" },
    { "src": "/api/verify", "dest": "/api/verify.js" },
    { "src": "/api/generate", "dest": "/api/generate.js" },
    { "src": "/api/stats", "dest": "/api/stats.js" }
  ]
}